doctype html
html(lang='en')
  head
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    title MSIN636 Projects Portfolio
    link(href='/vendor/bootstrap/css/bootstrap.min.css', rel='stylesheet')
    link(href='/vendor/fontawesome-free/css/all.min.css', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Montserrat:400,700', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic', rel='stylesheet', type='text/css')
    link(href='/vendor/magnific-popup/magnific-popup.css', rel='stylesheet', type='text/css')
    link(href='/stylesheets/freelancer.css', rel='stylesheet')
  body#page-top
    nav#mainNav.navbar.navbar-expand-lg.bg-secondary.fixed-top.text-uppercase
      .container
        a.navbar-brand.js-scroll-trigger(href='#page-top') MSIN636 Portfolio
        button.navbar-toggler.navbar-toggler-right.text-uppercase.bg-primary.text-white.rounded(type='button', data-toggle='collapse', data-target='#navbarResponsive', aria-controls='navbarResponsive', aria-expanded='false', aria-label='Toggle navigation')
          | Menu
          i.fas.fa-bars
        #navbarResponsive.collapse.navbar-collapse
          ul.navbar-nav.ml-auto
            li.nav-item.mx-0.mx-lg-1
              a.nav-link.py-3.px-0.px-lg-3.rounded.js-scroll-trigger(href='#portfolio') Portfolio
            li.nav-item.mx-0.mx-lg-1
              a.nav-link.py-3.px-0.px-lg-3.rounded.js-scroll-trigger(href='#about') About

    // Header
    header.masthead.bg-primary.text-white.text-center
      .container
        h1.text-uppercase.mb-0 MSIN636 Projects
        br
        h4.font-weight-light.mb-0 Node.js - MongoDB - Express - Bootstarp


    block content

    // Footer
    footer.footer.text-center
      .container
        .row
          .col-md-4.mb-5.mb-lg-0
            h4.text-uppercase.mb-4 Location
            p.lead.mb-0
              | 320 W 31st St,
              br
              |  New York, NY 10001
          .col-md-4.mb-5.mb-lg-0
            h4.text-uppercase.mb-4 Find more at
            ul.list-inline.mb-0
              li.list-inline-item
                a.btn.btn-outline-light.btn-social.text-center.rounded-circle(href='#')
                  i.fab.fa-fw.fa-facebook-f
              li.list-inline-item
                a.btn.btn-outline-light.btn-social.text-center.rounded-circle(href='#')
                  i.fab.fa-fw.fa-google-plus-g
              li.list-inline-item
                a.btn.btn-outline-light.btn-social.text-center.rounded-circle(href='#')
                  i.fab.fa-fw.fa-twitter
              li.list-inline-item
                a.btn.btn-outline-light.btn-social.text-center.rounded-circle(href='#')
                  i.fab.fa-fw.fa-linkedin-in
              li.list-inline-item
                a.btn.btn-outline-light.btn-social.text-center.rounded-circle(href='#')
                  i.fab.fa-fw.fa-dribbble
          .col-md-4
            h4.text-uppercase.mb-4 CONTACT US
            p.lead.mb-0
              | msin636@gmail.com
    .copyright.py-4.text-center.text-white
      .container
        small Copyright Â© Touro College
    // Scroll to Top Button (Only visible on small and extra-small screen sizes)
    .scroll-to-top.d-lg-none.position-fixed
      a.js-scroll-trigger.d-block.text-center.text-white.rounded(href='#page-top')
        i.fa.fa-chevron-up

    // Bootstrap core JavaScript

    script(src='vendor/jquery/jquery.min.js')
    script(src='vendor/bootstrap/js/bootstrap.bundle.min.js')
    script(src='vendor/jquery-easing/jquery.easing.min.js')
    script(src='vendor/magnific-popup/jquery.magnific-popup.min.js')
    script(src='javascripts/jqBootstrapValidation.min.js')
    script(src='javascripts/contact_me.js')
    script(src='javascripts/freelancer.min.js')
